&tlmm_pinmux {

    /delete-node/ spi2_active;
	/delete-node/ spi2_suspend;
	/delete-node/ spi2_cs2_active;
	/delete-node/ spi2_cs2_suspend;
	
	pmx_i2c_3 {
		/* CLK, DATA */
		qcom,pins = <&gp 11>, <&gp 10>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <2>;
		label = "pmx_i2c_3";

		i2c_3_active: i2c_3_active {
			drive-strength = <2>; /* 2 MA */
			bias-disable;		  /* No PULL */
		};

		i2c_3_sleep: i2c_3_sleep {
			drive-strength = <2>; /* 2 MA */
			bias-disable;		  /* No PULL */
		};
	};
	
	pmx_sdc2_clk {
				clk_on {
					bias-disable;
					drive-strength = <10>;
				};

				clk_off {
					bias-disable;
					drive-strength = <2>;
				};
	};
	
	pmx_sdc2_cmd {
				cmd_on {
					bias-pull-up;
					drive-strength = <10>;
				};

				cmd_off {
					bias-pull-up;
					drive-strength = <2>;
				};
	};
	
	pmx_mdss: pmx_mdss {
	      qcom,num-grp-pins = <1>;
		  qcom,pins = <&gp 17>;
	};
	
	tlmm_gpio_key {
	    qcom,pins = <&gp 107>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "tlmm_gpio_key";
	};
	
	lis3dh_int1_pin {
		qcom,pins = <&gp 21>;
		qcom,num-grp-pins = <1>;
		label = "lis3dh_int_pin";
		lis3dh_int1_default: int1_default {
			drive-strength = <6>;
			bias-pull-down;
		};
		lis3dh_int1_sleep: int1_sleep {
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	cdc-ext-spk-pa {
		qcom,pins = <&gp 120>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <0>;
		label = "cdc-ext-spk-pa";
		cdc_ext_spk_pa_act: ext_spk_pa_on {
			drive-strength = <8>;
			output-low;
			bias-pull-none;
		};
		cdc_ext_spk_pa_sus: ext_spk_pa_off {
			drive-strength = <2>;
			bias-disable;
		};
	};
	
	cdc-pdm-lines {
			pdm_lines_off {
			/delete-property/ bias-pull-down;
					bias-disable;
			};
	};

	goodix_ts_int {
		qcom,pins = <&gp 13>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "goodix_ts_int";

		gdix_ts_int_default: gdix_ts_int_default {
			drive-strength = <2>;
			input-enable;
			bias-disable;
		};

		gdix_ts_int_output_high: gdix_ts_int_output_high {
			output-high;
		};

		gdix_ts_int_output_low: gdix_ts_int_output_low {
			output-low;
		};

		gdix_ts_int_input: gdix_ts_int_input {
			input-enable;
			bias-disable;
		};
	};

	goodix_ts_rst {
		qcom,pins = <&gp 12>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "goodix_ts_rst";

		gdix_ts_rst_default: gdix_ts_rst_default {
			drive-strength = <2>;
			input-enable;
			bias-disable;
		};

		gdix_ts_rst_output_high: gdix_ts_rst_output_high {
			output-high;
		};

		gdix_ts_rst_output_low: gdix_ts_rst_output_low {
			output-low;
		};

		gdix_ts_rst_input: gdix_ts_rst_input {
			input-enable;
			bias-disable;
		};
	};

	usb-id-pin {
		qcom,pins = <&gp 110>;

		usbid_sleep: sleep {
			drive-strength = <8>;
			bias-disable = <0>;
		};
	};
    
    tsl2772x_int_pin {
			qcom,pins = <&gp 113>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "tsl2772x-irq";
			tsl2772x_default: tsl2772x_default {
				drive-strength = <6>;
				bias-pull-up;
			};
			tsl2772x_sleep: tsl2772x_sleep {
				drive-strength = <6>;
				bias-pull-up;
			};
		};
         
};
