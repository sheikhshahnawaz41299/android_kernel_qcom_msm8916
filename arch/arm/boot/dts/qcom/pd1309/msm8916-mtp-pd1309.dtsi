#include "dsi-panel-orise8012a_tmfwvga.dtsi"
#include "dsi-panel-orise8012a_tmfwvga_cmd_bkg.dtsi"
#include "dsi-panel-nt35510s_fwvga.dtsi"
#include "msm8916-camera-sensor-mtp-pd1309.dtsi"

/ {
	memory {
		pstore_reserve_mem: pstore_reserve_region@0 {
			linux,reserve-contiguous-region;
			linux,reserve-region;
			linux,remove-completely;
			reg = <0x0 0x8c400000 0x0 0x00100000>;
			label = "pstore_reserve_mem";
		};
        
        modem_adsp_mem: modem_adsp_region@0 {
			reg = <0x0 0x86800000 0x0 0x5500000>;
		};

		peripheral_mem: pheripheral_region@0 {
			reg = <0x0 0x8bd00000 0x0 0x600000>;
		};
	};

	aliases {
		i2c3 = &i2c_3; /* I2C3 controller device */
	};
};

&tlmm_pinmux {

    tlmm_gpio_key {
		        /delete-property/ qcom,num-grp-pins;
				/delete-property/ qcom,pins;
				qcom,pins = <&gp 107>;
				qcom,num-grp-pins = <1>;
		};
    pmx_mdss {
				/delete-property/ qcom,num-grp-pins;
				/delete-property/ qcom,pins;
				qcom,num-grp-pins = <1>;
				qcom,pins = <&gp 1>;
		};
    
    /delete-node/ spi2_active;
		/delete-node/ spi2_suspend;
		/delete-node/ spi2_cs2_active;
		/delete-node/ spi2_cs2_suspend;
        
    sdhc2_cd_pin {

				cd_on {
					bias-pull-up;
					bias-pull-down;
				};

				cd_off {
                /delete-property/ bias-disable;
					bias-pull-up;
					bias-pull-down;
				};
		};
    
    pmx_sdc2_clk {
				clk_on {
					drive-strength = <10>;
				};
		};
	
	pmx_sdc2_cmd {
				cmd_on {
					bias-pull-up;
					drive-strength = <10>;
				};
			};
    
	pmx_i2c_3 {
		/* CLK, DATA */
		qcom,pins = <&gp 11>, <&gp 10>;
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <2>;
		label = "pmx_i2c_3";

		i2c_3_active: i2c_3_active {
			drive-strength = <2>; /* 2 MA */
			bias-disable;		  /* No PULL */
		};

		i2c_3_sleep: i2c_3_sleep {
			drive-strength = <2>; /* 2 MA */
			bias-disable;		  /* No PULL */
		};
	};

	lis3dh_int1_pin {
		qcom,pins = <&gp 21>;
		qcom,num-grp-pins = <1>;
		label = "lis3dh_int_pin";
		lis3dh_int1_default: int1_default {
			drive-strength = <6>;
			bias-pull-down;
		};
		lis3dh_int1_sleep: int1_sleep {
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	cdc-ext-spk-pa {
		qcom,pins = <&gp 120>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <0>;
		label = "cdc-ext-spk-pa";
		cdc_ext_spk_pa_act: ext_spk_pa_on {
			drive-strength = <8>;
			output-low;
			bias-pull-none;
		};
		cdc_ext_spk_pa_sus: ext_spk_pa_off {
			drive-strength = <2>;
			bias-disable;
		};
	};

        
    cam_sensor_front_reset {
				qcom,pins = <&gp 28>;
				qcom,num-grp-pins = <1>;
				qcom,pin-func = <0>;
				label = "cam-sensor-front-reset";

				cam_sensor_front_reset_default:default {
					drive-strength = <2>;
					bias-disable = <0>;
					
				};
			};

			cam_sensor_front_reset_sleep {
				qcom,pins = <&gp 28>;
				qcom,num-grp-pins = <1>;
				qcom,pin-func = <0>;
				label = "cam-sensor-front-reset-sleep";

				cam_sensor_front_reset_sleep: sleep {
					drive-strength = <2>;
					bias-pull-down = <0>;
					
				};
			};

			cam_sensor_front_standby {
				qcom,pins = <&gp 33>;
				qcom,num-grp-pins = <1>;
				qcom,pin-func = <0>;
				label = "cam-sensor-front-standby";

			cam_sensor_front_standby_default: default {
					drive-strength = <2>;
					bias-disable = <0>;
					output-high;
					
				};
			};

			cam_sensor_front_standby_sleep {
				qcom,pins = <&gp 33>;
				qcom,num-grp-pins = <1>;
				qcom,pin-func = <0>;
				label = "cam-sensor-front-standby-sleep";

				cam_sensor_front_standby_sleep: sleep {
					drive-strength = <2>;
					bias-disable = <0>;
					
				};
			};
			
			cam_sensor_flash {
			    /delete-property/ qcom,num-grp-pins;
				/delete-property/ qcom,pins;
				qcom,pins = <&gp 31>, <&gp 32>;
				qcom,num-grp-pins = <2>;
			};

	usb-id-pin {
		qcom,pins = <&gp 110>;
		
		default {
					output-high;
				};

		usbid_sleep: sleep {
			drive-strength = <8>;
			bias-disable = <0>;
		};
	};
};

&soc {
	ramoops {
		compatible = "ramoops";
		status = "ok";

		android,ramoops-buffer-start = <0x8c400000>;
		android,ramoops-buffer-size = <0x100000>;
		android,ramoops-console-size = <0x40000>;
		android,ramoops-pmsg-size = <0x40000>;
		android,ramoops-record-size = <0x20000>;
		android,ramoops-dump-oops = <0x1>;
	};

	restart@4ab000 {
		qcom,download_mode-disable;
	};

	qcom,rmtfs_sharedmem@8e580000 {
		reg = <0x86690000 0x150000>;
	};

	qcom,wdt@b017000 {
		qcom,bark-time = <20000>;
		qcom,pet-time = <15000>;
	};

	qcom,ipc_router_modem_xprt {
		/delete-property/ qcom,disable-pil-loading;
	};

	qcom,smem@86300000 {
		qcom,smd-modem {
			/delete-property/ qcom,not-loadable;
		};
	};
	
	static-rules {
	    rule1 {
		   qcom,dest-bw = <900000>;
		};
		
		rule4 {
		   qcom,dest-bw = <900000>;
		};
	};
	

	i2c@78b8000 {
		smb358_otg_vreg: smb358-charger@57 {
			compatible = "qcom,smb358-charger";
			reg = <0x57>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <62 8>;
			pinctrl-names = "default";
			pinctrl-0 = <&smb_int_default>;

			qcom,float-voltage-mv = <4360>;
			qcom,recharge-mv = <100>;
			qcom,recharge-disabled;
			regulator-name = "smb358_otg_vreg";
			qcom,thermal-mitigation = <1300 700 600 0>;
			qcom,fastchg-current-max-ma = <1300>;

			qcom,disable-apsd;
			qcom,bms-psy-name = "bms";
			/* qcom,iterm-disabled; */
			qcom,iterm-ma = <150>;
			qcom,bms-controlled-charging;
			qcom,chg-inhibit-disabled;
			qcom,chg-vadc = <&pm8916_vadc>;
			qcom,chg-adc_tm = <&pm8916_adc_tm>;
			/* qcom,vcc-i2c-supply = <&pm8916_l5>; */
			qcom,irq-gpio = <&msm_gpio 62 0x00>;

			qcom,using-pmic-therm;
			qcom,hot-bat-decidegc = <550>;
			qcom,cold-bat-decidegc = <0>;
			qcom,bat-present-decidegc = <200>;
			/*
			 * Below properties are used for enable JEITA
			 */
			qcom,warm-bat-decidegc = <450>;
			qcom,cool-bat-decidegc = <0>;
			qcom,warm-bat-ma = <1000>;
			qcom,cool-bat-ma = <1000>;
			qcom,warm-bat-mv = <4000>;
			qcom,cool-bat-mv = <4000>;
			status = "ok";
		};
	};

	i2c_3: i2c@78b7000 { /* BLSP1 QUP3 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr", "bam_phys_addr";
		reg = <0x78b7000 0x1000>,
		      <0x7884000 0x23000>;
		interrupt-names = "qup_irq", "bam_irq";
		interrupts = <0 97 0>, <0 238 0>;
		qcom,clk-freq-out = <380000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
			<&clock_gcc clk_gcc_blsp1_qup3_i2c_apps_clk>;
		qcom,use-pinctrl;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_3_active>;
		pinctrl-1 = <&i2c_3_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		qcom,bam-pipe-idx-cons = <8>;
		qcom,bam-pipe-idx-prod = <9>;
		dmas = <&dma_blsp1 8 64 0x20000020 0x20>,
			<&dma_blsp1 9 32 0x20000020 0x20>;
		dma-names = "tx", "rx";
		qcom,master-id = <86>;
	};

	i2c@78b9000 { /* BLSP1 QUP5 */
		synaptics@20 {
			status = "ok";
		};

	};

	i2c@78ba000 { /* BLSP1 QUP6 */
		status = "okay";

		nfc-nci@e {
			status = "disabled";
		};
	};

	i2c@78b6000 { /* BLSP1 QUP2 */
		/delete-node/ mpu6050@68;
		/delete-node/ avago@39;
		/delete-node/ akm@c;

		taos@39 {
			compatible = "ams,tmd2772";
			reg = <0x39>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <113 0x2002>;
			pinctrl-names = "tmd2772_default","tmd2772_sleep";
			pinctrl-0 = <&apds99xx_default>;
			pinctrl-1 = <&apds99xx_sleep>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l6>;
			ams,irq-gpio = <&msm_gpio 113 0x2002>;
			ams,pul_cnt = <12>;
			ams,light-percent = <150>;
		};

		lis3dh_acc@18 {
			compatible = "st,lis3dh";
			reg = <0x18>;
			vdd-supply = <&pm8916_l17>;
			vddio-supply = <&pm8916_l6>;
			pinctrl-names = "lis3dh_default","lis3dh_sleep";
			pinctrl-0 = <&lis3dh_int1_default>;
			pinctrl-1 = <&lis3dh_int1_sleep>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <21 0x0>;
			st,min-interval = <10>;
			st,init-interval = <200>;
			st,axis-map-x = <1>;
			st,axis-map-y = <0>;
			st,axis-map-z = <2>;
			st,g-range = <2>;
			/*st,negate-x;*/ /*Disabled boolean property*/
			st,negate-y; /*Enabled boolean property*/
			/*st,negate-z;*/ /*Disabled boolean property*/
			st,gpio-int1 = <&msm_gpio 21 0x2002>;
			/*status = "disabled";*/
		};

		yamaha@2e {
			compatible = "yamaha,yas533";
			reg = <0x2e>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l6>;
			yas,position = <3>;
		};

	};

	qcom,msm-imem@8600000 {
		download_mode@0 {
			compatible = "qcom,msm-imem-download_mode";
			reg = <0x0 8>;
		};

		emergency_download_mode@fe0 {
			compatible = "qcom,msm-imem-emergency_download_mode";
			reg = <0xfe0 12>;
		};
	};

	sound {
		qcom,msm-mbhc-hphl-swh = <1>;
		qcom,msm-hs-micbias-type = "external";
		pinctrl-names = "cdc_lines_act",
				"cdc_lines_sus";
		pinctrl-0 = <&cdc_pdm_lines_act &cdc_ext_spk_pa_act>;
		pinctrl-1 = <&cdc_pdm_lines_sus &cdc_ext_spk_pa_sus>;
		/delete-property/ pinctrl-2;
		/delete-property/ pinctrl-3;
		/delete-property/ pinctrl-4;
		/delete-property/ pinctrl-5;
		/delete-property/ qcom,cdc-us-euro-gpios;
		qcom,msm-spk-ext-pa = <&msm_gpio 120 0>;
	};

	qcom,ion {
		qcom,ion-heap@21 {
			reg = <21>;
			qcom,ion-heap-type = "SYSTEM_CONTIG";
		};
	};

	gpio_keys {
		/delete-node/ camera_focus;
		/delete-node/ camera_snapshot;
	};

	spk_vreg: regulator_spk {
		compatible = "regulator-fixed";
		regulator-name = "spk_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&msm_gpio 121 0>;
	};
	
	qcom,venus@1de0000 {
			qcom,subsystem-restart-level;
		};
	
	qcom,mss@4080000 {
			qcom,subsystem-restart-level;
		};
	
	qcom,pronto@a21b000 {
			qcom,subsystem-restart-level;
		};
};

&pm8916_tombak_dig {
	cdc-vdd-spkdrv-supply = <&spk_vreg>;
	qcom,cdc-vdd-spkdrv-voltage = <5000000 5000000>;
	qcom,cdc-vdd-spkdrv-current = <20000>;
	qcom,cdc-on-demand-supplies = "cdc-vdd-mic-bias",
			"cdc-vdd-spkdrv";
};

&sdc2_cd_off {
	/delete-property/ bias-disable;
	bias-pull-up;
};

&sdc2_cmd_on {
	drive-strength = <12>;
};

&sdhc_1 {
	qcom,vdd-always-on;
};

&sdhc_2 {
	qcom,clk-rates = <400000 25000000 50000000 100000000>;
	cd-gpios = <&msm_gpio 38 1>;
};

&pm8916_pon {
	qcom,pon_1 {
		qcom,s2-type = <7>;
	};

	qcom,pon_2 {
		qcom,support-reset = <0>;
		qcom,s1-timer = <10256>;
		qcom,s2-timer = <2000>;
		qcom,s2-type = <1>;
	};
};

&pm8916_mpps {
	mpp@a100 { /* MPP 2 */
		/* HR LED */
		status = "ok";
		qcom,mode = <4>;
		qcom,ain-route = <1>;
		qcom,invert = <0>;
		qcom,output-type = <0>;
		qcom,src-sel = <0>;
		qcom,vin-sel = <0>;
		qcom,master-en = <1>;
	};

	mpp@a300 { /* MPP 4 */
		/* Backlight PWM */
		qcom,vin-sel = <3>;
	};
};

&pm8916_gpios {
	gpio@c100 { /* GPIO 2 */
		status = "disabled";
	};
};

&pm8916_rtc {
	qcom,qpnp-rtc-alarm-pwrup = <1>;
};

&pm8916_vadc {
	chan@11 {
		label = "usbin";
		reg = <0x11>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};
};


&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_orise8012a_tmfwvga>;
	qcom,platform-bklight-en-gpio = <&msm_gpio 8 0>;
};

/ {
	pd1309_batterydata: qcom,battery-data {
		/delete-node/ qcom,palladium-batterydata;

		#include "batterydata-b75-atl-4v35-2100mah.dtsi"
		#include "batterydata-desay-samsung-2260mAh-PD1410-VBMS.dtsi"
	};
};

&pm8916_chg {
	status = "ok";
	qcom,resume-soc = <95>;
	qcom,use-external-charger;
};

&pm8916_bms {
	status = "ok";
	qcom,resume-soc = <95>;
	qcom,battery-data = <&pd1309_batterydata>;
	qcom,max-voltage-uv = <4350000>;
	qcom,v-cutoff-uv = <3400000>;
	qcom,low-voltage-threshold = <3450000>;
	qcom,low-voltage-calculate-soc-ms = <30000>;
	qcom,low-soc-calculate-soc-ms = <70000>;
	qcom,calculate-soc-ms = <100000>;
};

&pm8916_leds {
	status = "disabled";
};

&spi_0 { /* BLSP1 QUP3 allocated for i2c_3 */
	status = "disabled";
};

&i2c_0 {
  qcom,clk-freq-out = <400000>;
};

&i2c_6 {
  status = "disabled";
};
